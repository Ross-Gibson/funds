{
  "name": "funds",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "yarn install && cd api && yarn install && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "if [[ \"$(${ANDROID_HOME-~/Library/Android/sdk}/platform-tools/adb devices | grep 'device$')\" != \"\" ]]; then ${ANDROID_HOME-~/Library/Android/sdk}/platform-tools/adb reverse tcp:8081 tcp:8081; fi; react-native start",
    "serve": "cd api && yarn start",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,jsx,tsx}'",
    "pods": "cd ios && pod install && cd ..",
    "pod-update": "cd ios && pod update && cd ..",
    "clean": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install && yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.6",
    "@react-navigation/bottom-tabs": "^5.0.5",
    "@react-navigation/core": "^5.1.4",
    "@react-navigation/native": "^5.0.5",
    "@react-navigation/native-stack": "^5.0.5",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-dark-mode": "^0.2.2",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-localization": "^2.1.6",
    "react-native-localize": "^1.3.3",
    "react-native-paper": "2.16.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-restart": "^0.0.13",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.0.0-alpha.34",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/react": "^16.9.20",
    "@types/react-native": "^0.61.15",
    "@types/react-native-restart": "^0.0.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-navigation": "^3.4.0",
    "@types/react-redux": "^7.1.7",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.9.0",
    "redux-devtools": "^3.5.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "preset": "react-native"
  }
